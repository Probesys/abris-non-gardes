                    <span class="brand-text font-weight-light">{{ 'Generics.labels.shortAppName'|trans() }}</span>
                </a>
                {% include 'admin/main-sidebar.html.twig' %}
                <!-- /.sidebar -->
            </aside>
            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Content Header (Page header) -->
                <div class="content-header">
                    <div class="container-fluid">
                        {% include 'admin/alerts.html.twig' %}
                        <div class="row mb-2">
                            <div class="col-sm-6">
                                <h1 class="m-0 text-dark">{% block title %}{% endblock title %}</h1>
                            </div>
                            <div class="col-sm-6">
                                <ol class="breadcrumb float-sm-right">
                                    <li class="breadcrumb-item"><a href="{{ path('admin_index') }}">{{ 'Generics.labels.homepage'|trans() }}</a></li>
                                        {% block breadcrumb %}

